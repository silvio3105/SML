{
    "configurations": [
        {
            "name": "GCC_ARM",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
				"DEBUG",
				"DEBUG_HANDLER=log",
				"DEBUG_LEVEL=0",
				"DEBUG_SML_RC",
				"DEBUG_SML_COPY"
            ],
			"compilerArgs": [
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARM-GCC}/arm-none-eabi-gcc.exe",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}